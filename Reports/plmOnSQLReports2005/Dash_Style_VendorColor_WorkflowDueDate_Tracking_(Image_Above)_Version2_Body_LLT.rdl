        </Field>
        <Field Name="CategoryLabel">
          <DataField>CategoryLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT     StyleCategoryID AS CategoryValue, StyleCategory AS CategorySort, StyleCategory AS CategoryLabel
FROM         pStyleCategory
WHERE     (StyleCategory IS NOT NULL)
GROUP BY StyleCategoryID, StyleCategory
UNION
SELECT     NULL, NULL, 'ALL'
ORDER BY CategorySort ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ds_information_users">
      <Fields>
        <Field Name="UserValue">
          <DataField>UserValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserSort">
          <DataField>UserSort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserLabel">
          <DataField>UserLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT 
	a.AssignedTo AS UserValue
	,  b.FirstName + ' ' + b.LastName AS UserSort
	,  b.FirstName + ' ' + b.LastName AS UserLabel

FROM pSampleRequestWorkflow a 
	LEFT OUTER JOIN Users b  ON  a.AssignedTo = b.UserId  
WHERE  (a.AssignedTo IS NOT NULL)   AND ( a.AssignedTo &lt;&gt; '')
UNION
SELECT   -999
	, NULL
	, 'ALL' 

ORDER BY  UserSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ds_information_categoryName">
      <Fields>
        <Field Name="StyleCategory">
          <DataField>StyleCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT StyleCategory FROM pStyleCategory WHERE (LTRIM(RTRIM( CAST(StyleCategoryID AS nvarchar(40)) )) = LTRIM(RTRIM(@StyleCategoryID ))) OR (@StyleCategoryID = 'ALL')</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StyleCategoryID">
            <Value>=Parameters!StyleCategoryID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ds_information_user_default">
      <Fields>
        <Field Name="UserId">
          <DataField>UserId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT TOP 1  b.UserId as UserId,  b.FirstName + ' ' + b.LastName as Name FROM pSampleRequestWorkflow a  INNER JOIN Users b  ON  a.AssignedTo = b.UserID  GROUP BY b.UserID, b.FirstName, b.LastName  ORDER BY b.FirstName, b.LastName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
