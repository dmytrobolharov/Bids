        </Field>
        <Field Name="AgentLabel">
          <DataField>AgentLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT 
	                RTRIM(CAST(TradePartnerID AS VARCHAR(40)))  AS   AgentValue
               ,TradePartnerName      AS   AgentSort
               ,TradepartnerName      AS   AgentLabel
    FROM    uTradePartner  
WHERE   RTRIM(CAST(TradePartnerID AS VARCHAR(40)))  IS NOT NULL    AND  LTRIM( RTRIM(TradePartnerName)) IS NOT NULL 
UNION
SELECT NULL, NULL, 'ALL'
ORDER BY AgentSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
