  <DataSets>
    <DataSet Name="ds_information_season">
      <Fields>
        <Field Name="SeasonValue">
          <DataField>SeasonValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SeanSortOrder">
          <DataField>SeanSortOrder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SeasonLabel">
          <DataField>SeasonLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT CustomField2 AS SeasonValue, CustomField2 AS SeanSortOrder, CustomField2 AS SeasonLabel
FROM            pStyleHeader
WHERE        (CustomField2 IS NOT NULL) AND (CustomField2 &lt;&gt; '')
UNION
SELECT        'ALL' AS Expr1, NULL AS Expr2, 'ALL' AS Expr3
ORDER BY SeanSortOrder</CommandText>
      </Query>
    </DataSet>
