    <DataSet Name="ParamQuery_ReminderOnly">
      <Fields>
        <Field Name="YesNoValue">
          <DataField>YesNoValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="YesNoSort">
          <DataField>YesNoSort</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="YesNoLabel">
          <DataField>YesNoLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT  1 AS  YesNoValue ,1    AS YesNoSort ,   'Yes'      AS  YesNoLabel
                                     UNION
                                     SELECT  0  AS  YesNoValue ,2    AS  YesNoSort ,   'No'      AS  YesNoLabel
       ORDER BY YesNoSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ParamQuery_Year">
      <Fields>
        <Field Name="YearValue">
          <DataField>YearValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearSort">
          <DataField>YearSort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearLabel">
          <DataField>YearLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT   Custom AS YearValue	, Custom AS YearSort	, Custom AS YearLabel  FROM pYear UNION 
                                  SELECT NULL, NULL,  'ALL'   ORDER BY YearSort ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ParamQuery_Season">
      <Fields>
        <Field Name="SeasonValue">
          <DataField>SeasonValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SeanSortOrder">
          <DataField>SeanSortOrder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SeasonLabel">
          <DataField>SeasonLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>
			SELECT distinct CustomKey as SeasonValue,Custom AS SeanSortOrder,Custom AS SeasonLabel  
			FROM pSeason where ltrim(rtrim(isnull(Custom,''))) != ''
			UNION
			SELECT     NULL, NULL, 'ALL'
			ORDER BY SeanSortOrder
		</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ParamQuery_StyleCategoryID">
      <Fields>
        <Field Name="CategoryValue">
          <DataField>CategoryValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategorySort">
          <DataField>CategorySort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryLabel">
          <DataField>CategoryLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT  DISTINCT
								RTRIM( CAST(StyleCategoryID  AS  VARCHAR(40)))    AS CategoryValue, StyleCategory AS CategorySort, StyleCategory AS CategoryLabel
                               FROM         pStyleCategory  WHERE     (StyleCategory IS NOT NULL)  GROUP BY StyleCategoryID, StyleCategory    UNION
                                SELECT     NULL, NULL, 'ALL'  ORDER BY CategorySort ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ParamQuery_UserID">
      <Fields>
        <Field Name="UserLabel">
          <DataField>UserLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserValue">
          <DataField>UserValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserSort">
          <DataField>UserSort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT  ISNULL(b.FirstName,'') + ' ' + ISNULL(b.LastName,'') AS UserLabel , a.AssignedTo as UserValue,  ISNULL(b.FirstName,'') + ' ' + ISNULL(b.LastName,'') AS UserSort 
                                     FROM pSampleRequestWorkflow a    LEFT OUTER JOIN Users b  ON  a.AssignedTo = b.UserId  
                                     WHERE  a.AssignedTo IS NOT NULL  and RTRIM(b.FirstName) IS NOT NULL
                                   UNION
                             SELECT  'ALL',  -999,  '0000'     ORDER BY  UserSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ParamQuery_TradePartnerID">
      <Fields>
        <Field Name="AgentValue">
          <DataField>AgentValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
