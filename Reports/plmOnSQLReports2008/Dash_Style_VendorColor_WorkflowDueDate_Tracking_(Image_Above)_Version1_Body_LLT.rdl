"  ~ ")

 &amp; Format( Parameters!DateTo.Value, "Short Date") &amp; 

IIF(Parameters!DateFrom.Value = "", "",
 ",")


 &amp; "    Year: " &amp; Parameters!Year.Value &amp;  ","  &amp; "   Season: "  &amp;  Parameters!Season.Value &amp;  ","  &amp; "   Item Category: " &amp;  




IIF(  Parameters!StyleCategoryID.Value="ALL" or Parameters!StyleCategoryID.Value="",
"ALL"
,
 First(Fields!StyleCategory.Value, "ds_information_categoryName") 

)
&amp;  ","  &amp; "   User: " &amp; 

IIF(Parameters!UserId.Value = -999, "ALL", First(Fields!UserFullName.Value, "ds_information_userName"))

&amp; ", Sort By:" &amp; IIF( Parameters!SortBy.Value=1, "Category", "Style No")</Value>
