  <Code>Public Function FormatDate(ByVal inputDate as Object) As String
	If inputDate Is Nothing Then
		Return String.Empty
	ElseIf	IsDate(inputDate) Then
		Return CDate(inputDate).ToShortDateString()
	Else
		Return String.Empty
	End If
End Function
Public Function Translate(ByVal theString As String) As String
   Return theString
End Function
