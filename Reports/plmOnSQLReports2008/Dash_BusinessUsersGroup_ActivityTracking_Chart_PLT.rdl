    <DataSet Name="ParamQuery_UserID">
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT DISTINCT  a.AssignedTo AS UserValue,  b.FirstName + ' ' + b.LastName AS UserSort,  b.FirstName + ' ' + b.LastName AS UserLabel
                                     FROM pSampleRequestWorkflow a    LEFT OUTER JOIN Users b  ON  a.AssignedTo = b.UserId  
                                     WHERE  (a.AssignedTo IS NOT NULL)       
                                   UNION
                             SELECT   NULL, NULL, 'ALL'    ORDER BY  UserSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
