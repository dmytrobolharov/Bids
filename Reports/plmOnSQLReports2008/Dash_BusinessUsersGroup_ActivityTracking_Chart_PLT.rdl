  <DataSets>
    <DataSet Name="ds_information_catergory">
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT        StyleCategoryId AS CategoryValue, StyleCategory AS CategorySort, StyleCategory AS CategoryLabel
FROM            pStyleCategory
WHERE        (StyleCategory IS NOT NULL)
GROUP BY StyleCategoryId, StyleCategory
UNION
SELECT        NULL AS Expr1, NULL AS Expr2, 'ALL' AS Expr3
ORDER BY CategorySort</CommandText>
      </Query>
      <Fields>
        <Field Name="CategoryValue">
          <DataField>CategoryValue</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="CategorySort">
          <DataField>CategorySort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryLabel">
