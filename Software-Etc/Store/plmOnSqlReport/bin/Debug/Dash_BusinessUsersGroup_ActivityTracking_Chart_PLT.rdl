</Value></Textbox></ReportItems><Height>0.5in</Height><PrintOnLastPage>true</PrintOnLastPage></PageHeader><BottomMargin>0.25in</BottomMargin><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">61fb8c29-b138-4818-8ede-66e4457f911c</rd:ReportID><DataSets><DataSet Name="ds_information_catergory"><Fields><Field Name="CategoryValue"><DataField>CategoryValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Guid</rd:TypeName></Field><Field Name="CategorySort"><DataField>CategorySort</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CategoryLabel"><DataField>CategoryLabel</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>plmOn</DataSourceName><CommandText>SELECT     StyleCategoryID AS CategoryValue, StyleCategory AS CategorySort, StyleCategory AS CategoryLabel
FROM         pStyleCategory
WHERE     (StyleCategory IS NOT NULL)
GROUP BY StyleCategoryID, StyleCategory
UNION
SELECT     NULL, NULL, 'ALL'
ORDER BY CategorySort ASC</CommandText></Query></DataSet><DataSet Name="ds_information_season"><Fields><Field Name="SeasonValue"><DataField>SeasonValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SeanSortOrder"><DataField>SeanSortOrder</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SeasonLabel"><DataField>SeasonLabel</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>plmOn</DataSourceName><CommandText>SELECT DISTINCT CustomField2 AS SeasonValue, CustomField2 AS SeanSortOrder, CustomField2 AS SeasonLabel
FROM         pStyleHeader
WHERE     (CustomField2 IS NOT NULL) AND (CustomField2 &lt;&gt; '')
UNION
SELECT     'ALL', NULL, 'ALL'
ORDER BY SeanSortOrder</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="ds_information_users"><Fields><Field Name="UserValue"><DataField>UserValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="UserSort"><DataField>UserSort</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="UserLabel"><DataField>UserLabel</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>plmOn</DataSourceName><CommandText>SELECT DISTINCT 
	a.AssignedTo AS UserValue
	,  b.FirstName + ' ' + b.LastName AS UserSort
	,  b.FirstName + ' ' + b.LastName AS UserLabel

FROM pSampleRequestWorkflow a 
	LEFT OUTER JOIN Users b  ON  a.AssignedTo = b.UserId  
WHERE  (a.AssignedTo IS NOT NULL)   AND ( a.AssignedTo &lt;&gt; '')
UNION
SELECT   -999
	, NULL
	, 'ALL' 

ORDER BY  UserSort</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="ds_information_user_default"><Fields><Field Name="UserID"><DataField>UserId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>plmOn</DataSourceName><CommandText>SELECT TOP 1  b.UserId as UserId,  b.FirstName + ' ' + b.LastName as Name FROM pSampleRequestWorkflow a  INNER JOIN Users b  ON  a.AssignedTo = b.UserID  GROUP BY b.UserID, b.FirstName, b.LastName  ORDER BY b.FirstName, b.LastName</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query></DataSet><DataSet Name="ds_information_year"><Fields><Field Name="YearValue"><DataField>YearValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="YearSort"><DataField>YearSort</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="YearLabel"><DataField>YearLabel</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Query><DataSourceName>plmOn</DataSourceName><CommandText>SELECT DISTINCT 
