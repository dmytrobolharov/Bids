      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>select  TOP 1  customField5, StyleCategory from pStyleHeader where (customField5 IS NOT NULL) AND (CustomField5 &lt;&gt; '') AND (CustomField5 &lt;&gt; '*n/a') and CustomField5 like '%Knit%'
