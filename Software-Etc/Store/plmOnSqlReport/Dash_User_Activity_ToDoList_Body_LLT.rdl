        </Field>
        <Field Name="CategoryLabel">
          <DataField>CategoryLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>plmOn</DataSourceName>
        <CommandText>SELECT     CustomField5 AS CategoryValue, CustomField5 AS CategorySort, CustomField5 AS CategoryLabel
FROM         pStyleHeader
WHERE     (CustomField5 IS NOT NULL) AND (CustomField5 &lt;&gt; '')
GROUP BY CustomField5
UNION
SELECT     'ALL', NULL, 'ALL'
ORDER BY CategorySort ASC</CommandText>
      </Query>
